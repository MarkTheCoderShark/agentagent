    background-clip: text;
    transform: scale(1.05);
}

.text-logo-small {
    font-size: 22px;
    font-weight: 700;
    color: #007bff;
    text-decoration: none;
    background: linear-gradient(135deg, #007bff 0%, #0056b3 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    letter-spacing: -0.5px;
}

.text-logo-mobile {
    font-size: 24px;
    font-weight: 700;
    color: #007bff;
    text-decoration: none;
    background: linear-gradient(135deg, #007bff 0%, #0056b3 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    letter-spacing: -0.5px;
}

/* Logo box adjustments */
.logo-box {
    display: flex;
    align-items: center;
}

.logo-box figure.logo {
    margin: 0;
}

.nav-logo {
    text-align: center;
    margin-bottom: 20px;
}

/* Hero Dashboard Mockup Styles */
.hero-dashboard {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 20px 0;
}

.dashboard-mockup {
    background: white;
    border-radius: 16px;
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.15);
    overflow: hidden;
    width: 100%;
    max-width: 500px;
    transform: perspective(1000px) rotateY(-5deg) rotateX(5deg);
    transition: all 0.3s ease;
}

.dashboard-mockup:hover {
    transform: perspective(1000px) rotateY(-2deg) rotateX(2deg) scale(1.02);
    box-shadow: 0 30px 80px rgba(0, 0, 0, 0.2);
}

.dashboard-header {
    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    color: white;
    padding: 20px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.dashboard-header .logo-small {
    font-size: 18px;
    font-weight: 700;
    color: white;
}

.header-right {
    display: flex;
    align-items: center;
    gap: 10px;
    font-size: 14px;
}

.status-indicator {
    width: 8px;
    height: 8px;
    border-radius: 50%;
    background: #4ade80;
    animation: pulse 2s infinite;
}

@keyframes pulse {
    0% { opacity: 1; }
    50% { opacity: 0.5; }
    100% { opacity: 1; }
}

.dashboard-content {
    padding: 25px;
    background: #f8fafc;
}

.agent-cards {
    margin-bottom: 25px;
}

.agent-card {
    background: white;
    border-radius: 12px;
    padding: 20px;
    margin-bottom: 15px;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);
    display: flex;
    align-items: center;
    gap: 15px;
    transition: all 0.3s ease;
}

.agent-card:hover {
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);
}

.agent-avatar {
    font-size: 32px;
    width: 50px;
    height: 50px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    border-radius: 12px;
    color: white;
}

.agent-info h4 {
    font-size: 16px;
    font-weight: 600;
    margin: 0 0 5px 0;
    color: #1f2937;
}

.agent-info p {
    font-size: 14px;
    color: #6b7280;
    margin: 0 0 10px 0;
}

.progress-bar {
    width: 100%;
    height: 6px;
    background: #e5e7eb;
    border-radius: 3px;
    overflow: hidden;
}

.progress {
    height: 100%;
    background: linear-gradient(90deg, #10b981 0%, #059669 100%);
    border-radius: 3px;
    transition: width 0.3s ease;
}

.metrics-panel {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 15px;
}

.metric {
    background: white;
    border-radius: 12px;
    padding: 20px;
    text-align: center;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);
    transition: all 0.3s ease;
}

.metric:hover {
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);
}

.metric-value {
    font-size: 24px;
    font-weight: 700;
    color: #1f2937;
    margin-bottom: 5px;
}

.metric-label {
    font-size: 12px;
    color: #6b7280;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

/* Responsive adjustments */
@media (max-width: 768px) {
    .dashboard-mockup {
        transform: none;
        max-width: 100%;
    }
    
    .dashboard-mockup:hover {
        transform: none;
    }
    
    .metrics-panel {
        grid-template-columns: 1fr;
        gap: 10px;
    }
    
    .agent-card {
        padding: 15px;
    }
    
    .agent-avatar {
        font-size: 24px;
        width: 40px;
        height: 40px;
    }
}
